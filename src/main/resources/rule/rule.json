{
    "type" : "position/download/request",//position则是定位这个（这个集合）元素，download则是下载操作,发送请求获取document
    "position" : {
      "type": "attribute/tag",
      "key" : "key",
      "value" : "value",
      "index" : 0 //-1表示获取全部
    },
    "download" : {
      "url": "url",
      "urlAttr": "src",//元素url的attr键
      "referer": null,//null表示使用默认值
      "path": null,
      "name": null,
      "downloadType": "imageDownload"//下载方式 有些方式需要特殊处理、默认imageDownload
    },
    "request": {
      "url": "src",//元素的attr键
      "convertType":"html",//转换类型。html转成document。json转成jsonObject。默认html
      "referer": null,//null表示使用默认值,
      "urlName": null //链接的名字，元素的attribute key
    },
    "nextStep": {
      //下一次的操作
    }
}